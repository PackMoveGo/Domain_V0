{
  "name": "packmovego_desktop",
  "version": "0.1.0",
  "type": "module",
  "description": "PackMoveGo Desktop Application - Moving and Packing Services",
  "main": "src/entry-server.jsx",
  "scripts": {
    "dev": "chmod +x scripts/turnOFFPort.sh && ./scripts/turnOFFPort.sh 5001 && vite --config config/vite.config.csr.ts --mode development --port 5001",
    "dev:react": "vite --config config/vite.config.csr.ts",
    "dev:redirect": "node scripts/http-redirect-server.js",
    "dev:secure": "concurrently -n redirect,vite -c cyan,magenta \"npm run dev:redirect\" \"chmod +x scripts/turnOFFPort.sh && ./scripts/turnOFFPort.sh 5001 && vite --config config/vite.config.csr.ts --mode development --port 5001\"",
    "certs:generate": "chmod +x scripts/generate-certs.sh && ./scripts/generate-certs.sh",
    "certs:trust": "chmod +x scripts/trust-cert.sh && ./scripts/trust-cert.sh",
    "certs:untrust": "chmod +x scripts/untrust-cert.sh && ./scripts/untrust-cert.sh",
    "dev:ssr": "npm run build:ssr && npm run preview:ssr",
    "build": "vite build --config config/vite.config.ssr.ts",
    "build:csr": "vite build --config config/vite.config.csr.ts",
    "build:ssr": "vite build --config config/vite.config.ssr.ts",
    "build:clean": "rm -rf dist",
    "build:analyze": "vite build --config config/vite.config.csr.ts --mode analyze",
    "build:profile": "vite build --config config/vite.config.csr.ts --mode profile",
    "preview": "npm run preview:csr",
    "preview:csr": "NODE_ENV=production vite build --config config/vite.config.csr.ts && vite preview --config config/vite.config.csr.ts --port 5001",
    "preview:ssr": "NODE_ENV=production node scripts/vite-ssr-preview.js",
    "preview:existing": "NODE_ENV=production node scripts/vite-ssr-preview.js",
    "preview:static": "npx serve dist -p 5001",
    "preview:build": "npm run build:both && npm run preview:ssr",
    "start": "NODE_ENV=production npm run build:csr && vite preview --config config/vite.config.csr.ts --port 5001",
    "start:prod": "node scripts/kill-port.js 5001 && NODE_ENV=production npm run build:csr && vite preview --config config/vite.config.csr.ts --port 5001",
    "server": "NODE_ENV=production node dist/server/assets/js/entry-server-*.js",
    "deploy": "npm run build:ssr && vercel --prod",
    "deploy:ssr": "npm run build:ssr && vercel --prod --yes",
    "dev:api:enable": "node scripts/dev-api-toggle.js enable",
    "dev:api:disable": "node scripts/dev-api-toggle.js disable",
    "dev:api:status": "node scripts/dev-api-toggle.js status",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "lint": "npx eslint . --config config/eslint.config.js --ext .ts,.tsx,.js,.jsx --fix",
    "type-check": "tsc --noEmit --project config/tsconfig.json",
    "perf:lighthouse": "npx lighthouse http://localhost:5001 --output html --output-path ./lighthouse-report.html",
    "audit:fix": "npm audit fix",
    "audit:check": "npm audit --audit-level=moderate",
    "clean:deps": "rm -rf node_modules package-lock.json && npm install",
    "kill:port": "chmod +x scripts/turnOFFPort.sh && ./scripts/turnOFFPort.sh"
  },
  "dependencies": {
    "@babel/plugin-transform-react-constant-elements": "^7.27.1",
    "@babel/plugin-transform-react-inline-elements": "^7.27.1",
    "@hookform/resolvers": "^5.0.1",
    "@tanstack/react-query": "^5.75.5",
    "@types/cors": "^2.8.18",
    "@vercel/analytics": "^1.5.0",
    "@vercel/speed-insights": "^1.2.0",
    "axios": "^1.7.9",
    "cors": "^2.8.5",
    "express": "^4.21.2",
    "invariant": "^2.2.4",
    "react": "^18.3.1",
    "react-calendar": "^5.1.0",
    "react-dom": "^18.3.1",
    "react-fast-compare": "^3.2.2",
    "react-ga4": "^2.1.0",
    "react-helmet-async": "^2.0.5",
    "react-hook-form": "^7.56.2",
    "react-router-dom": "^6.28.0",
    "zod": "^3.24.4"
  },
  "devDependencies": {
    "@eslint/js": "^9.17.0",
    "@testing-library/jest-dom": "^6.6.4",
    "@testing-library/react": "^16.3.0",
    "@types/express": "^4.17.23",
    "@types/jest": "^30.0.0",
    "@types/node": "^22.15.21",
    "@types/react": "^18.3.12",
    "@types/react-dom": "^18.3.1",
    "@vitejs/plugin-react": "^4.2.1",
    "autoprefixer": "^10.4.17",
    "concurrently": "^8.2.2",
    "cssnano": "^7.0.7",
    "eslint": "^9.17.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^5.1.0",
    "eslint-plugin-react-refresh": "^0.4.16",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "postcss": "^8.4.49",
    "postcss-nested": "^7.0.2",
    "rollup-plugin-visualizer": "^5.14.0",
    "tailwindcss": "^3.4.1",
    "terser": "^5.43.1",
    "ts-jest": "^29.4.0",
    "typescript-eslint": "^8.18.2",
    "vite": "^6.3.5"
  },
  "engines": {
    "node": ">=18.14.0",
    "npm": ">=8.0.0"
  },
  "funding": {
    "type": "investor",
    "url": "https://api.packmovego.com/investor"
  },
  "keywords": [
    "moving",
    "packing",
    "services",
    "react",
    "vite",
    "typescript",
    "ssr"
  ],
  "author": "PackMoveGo Team",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/PackMoveGo/Domain_V0.git"
  },
  "bugs": {
    "url": "https://github.com/packmovego/Domain_V0/issues"
  },
  "homepage": "https://packmovego.com"
}
