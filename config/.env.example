# Environment Configuration Example
# Copy this file to .env.development.local or .env.production.local
# DO NOT commit .env*.local files to Git
# All frontend variables must use VITE_ prefix to be accessible in the browser
# IMPORTANT: Replace all placeholder values with your actual configuration

NODE_ENV=development

# Frontend Configuration (VITE_ prefix required for client-side access)
# Replace with your actual API URL
VITE_API_URL=https://api.example.com
VITE_SKIP_BACKEND_CHECK=false
VITE_DEV_HTTPS=false
VITE_DEV_MODE=development
VITE_MODE=development
VITE_APP_NAME=YourAppName
VITE_APP_VERSION=0.1.0
VITE_REDUCE_LOGGING=false
VITE_ENABLE_DEV_TOOLS=true

# API Configuration
VITE_API_TIMEOUT=10000
VITE_API_RETRY_ATTEMPTS=3
VITE_API_RETRY_DELAY=1000

# Cache Configuration
VITE_CACHE_ENABLED=true
VITE_CACHE_TTL=3600
VITE_CACHE_MAX_SIZE=100

# API Key (REPLACE WITH YOUR ACTUAL API KEY - Never commit real keys!)
# Generate a secure API key for your application
VITE_API_KEY_FRONTEND=REPLACE-WITH-YOUR-ACTUAL-API-KEY
VITE_API_KEY_ENABLED=false

# Note: Backend-only variables (JWT_SECRET, CORS_ORIGIN, SIGNIN_HOST, etc.)
# should be configured in the backend .env files, not here.
